{
  "name": "poc-anim",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "rollup --config --watch",
    "clean": "shx rm -rf dist && shx mkdir dist",
    "copy": "shx cp src/index.html dist/index.html",
    "html": "npx html-minifier --input-dir dist --output-dir dist --collapse-whitespace",
    "server": "npx live-server src",
    "start": "npm-run-all --parallel watch server",
    "build": "rollup --config --environment NODE_ENV:production",
    "prod": "npm-run-all clean copy html build"
  },
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.4.0",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.78.1",
    "rollup-plugin-styles": "^4.0.0",
    "sass": "^1.54.5",
    "shx": "^0.3.4",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@ionic/core": "git+https://github.com/MEDGRUPOGIT/med-components.git#1.6.5"
  }
}
